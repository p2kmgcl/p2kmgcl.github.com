{
  "name": "pablo-molina",
  "title": "Pablo Molina",
  "description": "Pablo Molina's website with links to his social network profiles, contact information and projects",
  "version": "1.0.0",
  "license": "see LICENSE.md",
  "author": {
    "name": "Pablo Molina",
    "email": "contact@pablomolina.me",
    "url": "https://pablomolina.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/p2kmgcl/p2kmgcl.github.com"
  },
  "main": "src/index.pug",
  "config": {
    "favicon": {
      "default": 192,
      "manifest": [
        36,
        48,
        72,
        96,
        144,
        192,
        512
      ],
      "icon": [
        16,
        32,
        96,
        192
      ],
      "appleTouchIcon": [
        57,
        60,
        72,
        76,
        114,
        120,
        144,
        152,
        180
      ],
      "msapplication": [
        144
      ]
    },
    "backgroundColor": "#fffcdc",
    "themeColor": "#d9a7c7"
  },
  "scripts": {
    "analyze-bundle": "rimraf ./build/ && webpack-cli --config internals/webpack-bundle-analyzer.js --progress",
    "build": "rimraf ./build/ && webpack-cli --config internals/webpack-prod.js --progress",
    "typecheck": "tsc --allowJs true --checkJs true --esModuleInterop true --module esnext --moduleResolution node --noEmit true --pretty true --resolveJsonModule true --strict true --experimentalDecorators true --target esnext",
    "typecheck:all": "npm run typecheck -- $(git ls-files | grep \".\\(js\\)$\")",
    "format": "prettier --check --write --config .prettierrc.json",
    "format:all": "npm run format -- $(git ls-files)",
    "start": "rimraf ./build && webpack --config internals/webpack-dll.js && webpack-dev-server --config internals/webpack-dev.js",
    "start:production": "http-server -d false -g true ./build",
    "test": "jest --config .jestrc.json --passWithNoTests"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/copy-webpack-plugin": "^4.4.4",
    "@types/expect-puppeteer": "^3.3.1",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/http-server": "^0.10.0",
    "@types/jest": "^24.0.13",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/mkdirp": "^0.5.2",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/puppeteer": "^1.12.4",
    "@types/sharp": "^0.22.1",
    "@types/webpack": "^4.4.27",
    "@types/webpack-bundle-analyzer": "^2.13.1",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-inline-css-webpack-plugin": "^1.4.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-puppeteer": "^4.1.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "mkdirp": "^0.5.1",
    "offline-plugin": "^5.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "puppeteer": "^1.16.0",
    "resource-hints-webpack-plugin": "0.0.2",
    "rimraf": "^2.6.3",
    "sharp": "^0.22.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.4",
    "url-loader": "^1.1.2",
    "webpack": "~4.28.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "dependencies": {
    "nanoid": "^2.0.3",
    "normalize.css": "^8.0.1"
  }
}
